// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
      "**/dist/**/*": true,
      "**/*.lock": true
    },
    "editor.tabSize": 2,
    "typescriptHero.codeOutline.enabled": true,
    "typescriptHero.resolver.multiLineWrapThreshold": 10,
    "typescriptHero.resolver.multiLineTrailingComma": true,
    "typescriptHero.resolver.organizeOnSave": true,
    "tslint.configFile": "tslint.json",
    "tslint.enable": true,
    "cSpell.words": [
      "Rewriteable"
    ],
    "tslint.packageManager": "yarn",
    "npm.packageManager": "yarn"
  }